{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "rootDir": ".",
    "outDir": "dist",
    "removeComments": true,
    "strict": false,
    "moduleResolution": "node",
    "baseUrl": "./",
    "pretty": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@enums": ["src/enums"],
      "@interfaces": ["src/interfaces"],
      "@services": ["src/services"],
      "@types": ["src/typess"],
      "@utils": ["src/utils"],
    }
  },
  "exclude": [
    "node_modules",
  ],
  "typedocOptions": {
    "entryPoints": [
      "src/enums/index.ts",
      "src/interfaces/index.ts",
      "src/types/index.ts"
    ],
    "out": "dist/docs",
    "exclude": [
      "../node_modules",
      "src/**/*.spec.ts",
      "./jest.config.ts"
    ],
    "highlightTheme": "light-plus",
    "name": "API Documentation",
    "hideGenerator": true
  },
}